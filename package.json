{
  "name": "client-side-react-server-boilerplate",
  "version": "1.0.0",
  "description": "Client side server react boilerplate implementation that includes jest, enzyme, lint, prettier, hot module reload, redux, thunk, router and etc. Feel free to fork it.",
  "main": "src/client/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/antonkalik/client-side-react-server-boilerplate.git"
  },
  "scripts": {
    "start": "npm run prod:build && node dist/server_bundle.js",
    "prod:build": "concurrently \"npm run prod:server\" \"npm run prod:client\"",
    "prod:server": "webpack --config ./webpack.server.js --mode production",
    "prod:client": "webpack --config ./webpack.client.js --mode production",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon ./src/server/index.js",
    "dev:client": "webpack-dev-server --mode development --config ./webpack.client.js --open",
    "test": "jest --config ./jest.config.js",
    "lint": "eslint ./src -c .eslintrc.json",
    "index": "node ./scripts/generate-indexes.js -all",
    "new": "node ./scripts/new.js",
    "del": "node ./scripts/delete.js",
    "list": "node scripts/list.js"
  },
  "author": "anton kalik",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/helper-compilation-targets": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.4.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "faker": "^4.1.0",
    "file-loader": "^5.0.2",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prompt": "^1.0.0",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^2.3.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
